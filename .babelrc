{
  "plugins": ["transform-object-rest-spread", "transform-class-properties"],
  "presets": [
    "flow",
    ["env", {
      "useBuiltIns": true,
      "modules": "commonjs",
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "env": {
    "es": {
      "plugins": ["transform-object-rest-spread", "transform-class-properties"],
      "presets": [
        "flow",
        ["env", {
          "useBuiltIns": true,
          "modules": false,
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }]
      ]
    }
  }
}
